<?xml version="1.0" encoding="windows-1251" standalone="no"?>
<DECLAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:noNamespaceSchemaLocation="J1603101.xsd">
    <DECLARHEAD>
        <TIN>{{ data.code }}</TIN>
        <C_DOC>J16</C_DOC>
        <C_DOC_SUB>031</C_DOC_SUB>
        <C_DOC_VER>1</C_DOC_VER>
        <C_DOC_TYPE>200</C_DOC_TYPE>
        <C_DOC_CNT>{{ doc_num }}</C_DOC_CNT>
        <C_REG>{{ now.day }}</C_REG>
        <C_RAJ>{{ str(now.year)[-2:] }}</C_RAJ>
        <PERIOD_MONTH>{{ now.month }}</PERIOD_MONTH>
        <PERIOD_TYPE>5</PERIOD_TYPE>
        <PERIOD_YEAR>{{ now.year }}</PERIOD_YEAR>
        <C_STI_ORIG>101</C_STI_ORIG>
        <C_DOC_STAN>3</C_DOC_STAN>
        <LINKED_DOCS xsi:nil="true"/>
        <D_FILL>{{ now.strftime('%d%m%Y') }}</D_FILL>
        <SOFTWARE>Integration Bot</SOFTWARE>
    </DECLARHEAD>
    <DECLARBODY>
        <HNUM>{{ request_id }}</HNUM>
        <HNAME>Мирний Олександр Володимирович</HNAME>
        <HTIN>1010101017</HTIN>
        <HKSTI>2659</HKSTI>
        <HSTI>ДПI у Шевченківському районі ГУ ДФС у м.Києві</HSTI>
        <R0101G1S>{{ data.tender_id }}</R0101G1S>
        <R0201G1S>{{ data.code }}</R0201G1S>
        <R0202G1S>{{ data.name }}</R0202G1S>
        {% if data.is_physical %}
        <R0203G1S>{{ data.first_name }}</R0203G1S>
        <R0204G1S>{{ data.last_name }}</R0204G1S>
        <R0205G1S>{{ data.family_name }}</R0205G1S>
        {% endif %}
        <HFILL>{{ now.strftime('%d%m%Y') }}</HFILL>
        <HTIME>{{ now.strftime('%H:%M:%S') }}</HTIME>
    </DECLARBODY>
</DECLAR>